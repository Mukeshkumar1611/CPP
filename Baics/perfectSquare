// Perfect Square

#include <iostream>
#include <math.h>
using namespace std;

void checkperfect(int n)
{
    int sqr = sqrt(n);
    if (sqr * sqr == n)
    {
        cout << "Perefect Square";
    }
    else
    {
        cout << "Not a Perfect sqaure";
    }
}

int main()
{
    int n;
    cout << "Enter Number : ";
    cin >> n;
    checkperfect(n);
}
