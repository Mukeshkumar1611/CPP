
#include <iostream>
using namespace std;

class Node
{
public:
    int data;
    Node *next;
    Node *previous;
};

bool search(Node *head, int key)
{
    Node *currNode = head;
    while (currNode->next != NULL)
    {
        if (currNode->data == key)
        {
            return true;
        }
        currNode = currNode->next;
    }
    return false;
}

void printList(Node *head)
{
    while (head != NULL)
    {
        cout << head->data << " ";
        head = head->next;
    }
}

int main()
{
    Node *head = new Node();
    Node *second = new Node();
    Node *third = new Node();

    head->previous = NULL;
    head->data = 1;
    head->next = second;

    second->previous = head;
    second->data = 2;
    second->next = third;

    third->previous = second;
    third->data = 3;
    third->next = NULL;

    printList(head);
    cout << endl;
    cout << search(head, 4);

    return 0;
}
