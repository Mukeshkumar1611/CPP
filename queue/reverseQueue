// Reverse queue

#include <iostream>
#include <stack>
#include <queue>
using namespace std;

void reverseQueue(queue<int> &q)
{
    stack<int> s1;
    while (!q.empty())
    {
        int front = q.front();
        s1.push(front);
        q.pop();
    }
    while (!s1.empty())
    {
        int fronte = s1.top();
        q.push(fronte);
        s1.pop();
    }
}

int main()
{
    queue<int> q;
    int size;
    cout << "Enter size : ";
    cin >> size;

    cout << "Enter elements : ";

    for (int i = 0, val; i < size; i++)
    {
        cin >> val;
        q.push(val);
    }

    reverseQueue(q);
    cout << "Reversed queue : ";
    while (!q.empty())
    {
        cout << q.front() << " ";
        q.pop();
    }

    cout << "\n";
}
