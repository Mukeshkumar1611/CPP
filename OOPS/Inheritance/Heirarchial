// Heirarchial Inhertance

#include <iostream>
using namespace std;

class A
{
public:
    void abc()
    {
        cout << "Parent1 Class" << endl;
    }
};

class B : public A
{
public:
    void abd()
    {
        cout << "child class " << endl;
    }
};

class C : public A
{
public:
    void abe()
    {
        cout << "child2 " << endl;
    }
};

int main()
{
    C obj;
    obj.abc();

    B obj1;
    obj1.abc();
    return 0;
}
