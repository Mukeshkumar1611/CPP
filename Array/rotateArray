#include <iostream>
using namespace std;
int main()
{
    int n;
    cout << "Enter size of array : ";
    cin >> n;
    int arr[n];
    cout << "Enter elements : ";
    for (int i = 0; i < n; i++)
    {
        cin >> arr[i];
    }
    int d;
    cout << "Enter numbeer to rotate array :";
    cin >> d;
    cout << "Your updated array : ";
    for (int i = 0; i < n; i++)
    {
        cout << arr[(i + d) % n] << " ";
    }
    return 0;
}
