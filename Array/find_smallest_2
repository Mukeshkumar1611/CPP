#include<iostream>
using namespace std;

int main()
{
    int n;
    cout << "Enter Size of Array: " << endl;
    cin >> n;
    int arr[n];

    for(int i=0;i<n;i++){
        cin >> arr[i];
    }

    int smallest = arr[0];

    for(int i=1;i<5;i++){
        if(arr[i] < smallest){
            smallest = arr[i];
        }
    }
    cout <<"Smallest Element of Array: " <<smallest << endl;
    return 0;
}
