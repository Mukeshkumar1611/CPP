#include <iostream>
using namespace std;
int main()
{
    int arr[50];
    int n;
    cout << "Enter size of array : ";
    cin >> n;
    cout << "Enter elements : ";
    for (int i = 0; i < n; i++)
    {
        cin >> arr[i];
    }

    int x;
    cout << "Enter the element you want to insert : ";
    cin >> x;

    int mid = n / 2;
    int i = n - 1;

    while (i >= mid)
    {
        arr[i + 1] = arr[i];
        i--;
    }
    n = n + 1;
    arr[mid] = x;

    for (int i = 0; i < n; i++)
    {
        cout << arr[i] << " ";
    }

    return 0;
}
